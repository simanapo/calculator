{
  "manifest_version": 2,
  "version": "1.0.3",
  "name": "シンプル電卓機 Simple Calculator",
  "description": "電卓として使うことができます。ちょっとした計算や確認がすぐに実行できて便利です。",
  "browser_action": {
    "default_icon": {
      "19": "images/19.png",
      "38": "images/38.png"
    },
    "default_title": "シンプル電卓機",
    "default_popup": "index.html"
  },
  "icons":{
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "sandbox":{
    "pages": [
        "index.html"
    ]
}
}